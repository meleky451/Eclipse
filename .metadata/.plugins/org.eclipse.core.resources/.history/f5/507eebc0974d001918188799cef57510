package calss11;

public class RetrievingAllValues {

	public static void main(String[] args) {
		
		
		int [][] num= {
				
				{11, 12,13, 14},
				{21, 22, 23,},
				{31, 32, 33, 34}
		
		};
		
		int rows=num.length;
		
		System.out.println(rows);
		
		int cols=num[1].length;
		System.out.println(cols);
		
	}

}
