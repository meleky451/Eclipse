package calss11;

public class Task3 {

	public static void main(String[] args) {
		
		
		String[][] groups= {
                {"Mr", "Mrs", "Ms", "Miss"},
                {"Smith", "Obama", "Jackson", "Jordan"},
                };
		
		 System.out.println(groups[0][1]+ " "+ groups [0][0]);
		 System.out.println(groups[0][0]+ " "+ groups [0][2]);
		 System.out.println(groups[0][2]+ " "+ groups [1][1]);
		 System.out.println(groups[0][3]+ " "+ groups [1][3]);
        
	}

}
