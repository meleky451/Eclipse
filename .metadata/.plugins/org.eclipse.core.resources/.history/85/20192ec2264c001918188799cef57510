package OnlineClasss20march;

public class excercises9 {

	public static void main(String[] args) {
		

		for (int i=1; i<5; i++) {
			for (int y=1; y<5; y++) {
				
				if(i==y) {
					System.out.println(i);
					
				}else {
					System.out.println(".");
				}
				System.out.print();
			}
		}

		
		
		
	
	
	
		
	}

}
