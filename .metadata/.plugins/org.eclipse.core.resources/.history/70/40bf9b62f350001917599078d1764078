package class12;

public class Task15 {

	public static void main(String[] args) {
		
		
//	    Find the number of values above the value of 7.
	       int num = 7;
	       int count = 0;
	       for (int row = 0; row < rating.length; row++) {
	           for (int col = 0; col < rating[row].length; col++) {
	               if (rating[row][col] > num)
	                   count++;
	           }
	       }
	       

	}

}
