package class4;

import java.util.Scanner;

public class excercise1 {

	public static void main(String[] args) {
		
	Scanner input=new Scanner (System.in);
	
	System.out.println("Kac paraya ihtiyacin var");
	
	double borc=input.nextDouble();
	 if(borc<2000) {
		 System.out.println("parayi kabul etik");
	}else {
		System.out.println("parayi kabul etmedik");
	}
	
	
	
	}

}
